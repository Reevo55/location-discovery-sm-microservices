{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/reels/src",
  "monorepo": true,
  "root": "apps/reels",
  "entryFile": "/src/main",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/reels/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "reels": {
      "type": "application",
      "root": "apps/reels",
      "entryFile": "main",
      "sourceRoot": "apps/reels/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reels/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "files": {
      "type": "application",
      "root": "apps/files",
      "entryFile": "main",
      "sourceRoot": "apps/files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files/tsconfig.app.json"
      }
    },
    "reactions": {
      "type": "application",
      "root": "apps/reactions",
      "entryFile": "main",
      "sourceRoot": "apps/reactions/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reactions/tsconfig.app.json"
      }
    },
    "weather": {
      "type": "application",
      "root": "apps/weather",
      "entryFile": "main",
      "sourceRoot": "apps/weather/src",
      "compilerOptions": {
        "tsConfigPath": "apps/weather/tsconfig.app.json"
      }
    },
    "posts": {
      "type": "application",
      "root": "apps/posts",
      "entryFile": "main",
      "sourceRoot": "apps/posts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/posts/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    }
  }
}